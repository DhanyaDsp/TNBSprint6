<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@drawable/checkbox_selected" android:state_checked="true" /> <!-- checked -->
    <item android:state_checked="false"> <!-- unchecked-->
        <shape >
            <stroke
                android:width="1dp"
                android:color="@android:color/white" />
        </shape>
    </item>
</selector>